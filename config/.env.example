# Example environment configuration
# Copy this file to .env and modify as needed

# Common settings
common.environment=development  # Options: development, staging, production
common.log_level=info           # Options: debug, info, warn, error
common.debug=false              # Enable debug mode: true/false

# Server settings
server.port=8080                # HTTP port to listen on
server.host=0.0.0.0             # Host to bind to

# Add service-specific settings below as needed

# RabbitMQ settings
rabbitmq.host=rabbitmq          # RabbitMQ host
rabbitmq.port=5672              # RabbitMQ port
rabbitmq.username=qubool        # RabbitMQ username
rabbitmq.password=qubool123      # RabbitMQ password